{{#section 'head'}}
<!-- stuff that goes in head...example: -->
<link href="/CSS/member.css" rel="stylesheet">
{{/section}}

<main class="container custom-flex my-5">
    <div class="friendRequestCard bg-555 my-5">
        <h1 class="has-text-centered my-4 is-size-2">{{note.title}}</h1>
        <div class="line mb-3"></div>
        <p class="mx-5 pb-5">{{note.description}}</p>
    </div>
</main>

<div class="custom-flex mb-3">
    <audio controls>
        <source src={{note.song.file_path}} type="audio/mpeg">
    </audio>
</div>

<div class="custom-flex">
    <a href="/userProfile" class="button is-primary">Go back</a>
</div>